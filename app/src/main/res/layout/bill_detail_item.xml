<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="2dp"
    android:background="@drawable/layout_round"
    >
    <TextView
        android:id="@+id/masach"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="masach"
        android:textColor="@color/colorBlack"
        />
    <TextView
        android:id="@+id/soluong"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="số lượng"
        android:textColor="@color/colorBlack"
        android:layout_below="@+id/masach"
        />
    <TextView
        android:id="@+id/giabia"
        android:textColor="@color/colorBlack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="giá bìa"
        android:layout_below="@+id/soluong"

        />
    <TextView
        android:id="@+id/money"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="thành tiền"
        android:textColor="@color/colorBlack"
        android:layout_below="@+id/giabia"

        />

    <Button
        android:id="@+id/image_del"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_margin="10dp"
        android:layout_alignParentRight="true"
        android:background="@drawable/ic_delete_forever_black_24dp"
        />


</RelativeLayout>